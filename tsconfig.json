{
  "compilerOptions": {
    "baseUrl": ".",
		"paths": {
			"components/*": ["src/components/*"],
			"config/*": ["src/config/*"],
			"styles/*": ["src/styles/*"],
			"utils/*": ["src/utils/*"],
			"assets/*": ["src/assets/*"],
			"store/*": ["src/store/*"],
			"data/*": ["src/data/*"]
		},
    "noEmit": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "module": "ES6",
    "target": "ES6",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}